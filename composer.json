{
    "minimum-stability": "dev",
    "prefer-stable": true,
    "name": "for/http-middleware-psr15-adapter",
    "description": "PSR15 adapter for react/http middleware",
    "keywords": ["HTTP", "HTTPS", "ReactPHP", "middleware", "PSR15"],
    "license": "MIT",
    "require": {
        "php": "^7.0 || ^5.6.0",
        "http-interop/http-middleware": "^0.4.1",
        "recoil/react": "^1.0@alpha",
        "recoil/recoil": "^1.0@alpha",
        "nikic/php-parser": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "FriendsOfReact\\Http\\Middleware\\Psr15Adapter\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.10||^5.0"
    }
}
